{
  "name": "react18-vite-tailwind-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "fix:eslint": "eslint --fix ./src",
    "fix:prettier": "prettier --write ./src",
    "lint:eslint": "eslint --cache ./src",
    "lint:prettier": "prettier --check ./src",
    "test": "npx vitest --run",
    "test:run": "npx vitest --run",
    "test:watch": "npx vitest",
    "test:coverage": "npx vitest --coverage",
    "e2e": "npx playwright test",
    "e2e:head": "npx playwright test --headed",
    "e2e:chromium": "npx playwright test --project=chromium",
    "e2e:firefox": "npx playwright test --project=firefox",
    "e2e:webkit": "npx playwright test --project=webkit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^1.3.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__user-event": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-import-access": "^2.1.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "happy-dom": "^4.1.0",
    "husky": "^8.0.1",
    "jest-extended": "^2.0.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^13.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.13.1"
  },
  "volta": {
    "node": "18.3.0"
  }
}
